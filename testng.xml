<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<listeners>
		<listener class-name="pallavi.com.SeleniumFramework.testcomponents.TestListener"/>
	</listeners>
  <test thread-count="2" name="Products">
    <classes>
      <class name="pallavi.com.SeleniumFramework.tests.ProductsTest"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="2" name="ErrorValidations">
    <classes>
      <class name="pallavi.com.SeleniumFramework.tests.ErrorValidationsTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
